#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#define MAX 1001


int main() {
    
    char expressao[MAX];
    int parentese, tamanho, n;
    for (n = 0; n < 10000; n++) {
        parentese = 0;
        if (scanf("%s", expressao) == EOF) {
            break; // final do arquivo
        }
        tamanho = strlen(expressao);
        for (int i = 0; i < tamanho; i++) {
            if (expressao[i] == '(') {
                parentese++;
            } else if (expressao[i] == ')' && parentese != 0) {
                parentese--;
            } else if (expressao[i] == ')' && parentese == 0) {
                parentese = -1;
                break;
            }
        }
        if (parentese == 0) {
            printf("correct\n");
        } else {
            printf("incorrect\n");
        }
    }

    return 0;
}
