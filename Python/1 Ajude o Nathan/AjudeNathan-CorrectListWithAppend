testes = int(input())
output = []

for i in range(testes):
    k, t, n = map(int, input("Digite três valores inteiros separados por espaços: ").split())
    
    resposta1 = t + n
    resposta2 = resposta1 // k
    
    if resposta2 % 2 == 0:
        output.append("Thiago")
    else:
        output.append("Nathan")

# Imprime o output completo
print("\n".join(output))
