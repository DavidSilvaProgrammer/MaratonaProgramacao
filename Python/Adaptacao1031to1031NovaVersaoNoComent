def sequencia_primos(n):
    primos = []
    num = 2
    while len(primos) < n:
        is_primo = True
        for p in primos:
            if num % p == 0:
                is_primo = False
                break
        if is_primo:
            primos.append(num)
        num += 1
    return primos

while True:
    n = int(input())
    if n == 0:
        break

    primos = sequencia_primos(n)  
    m = primos[0]
    while True:
        regions = list(range(1, n+1))
        pos = 0
        regions.pop(pos)
        while len(regions) > 1:
            pos = (pos + m - 1) % len(regions)
            regions.pop(pos)
        if len(regions) == 1:  
            print(regions[0])
            break
        m = primos[(pos + 1) % n]

