import sys

while True:
    expressao = sys.stdin.readline().strip()
    if len(expressao) > 0 and len(expressao) <= 1000:
        parentese = 0
        for caractere in expressao:
            if caractere == "(":
                parentese += 1
            elif caractere == ")" and parentese != 0:
                parentese -= 1
            elif caractere == ")":
                parentese = -1
                break
        if parentese == 0:
            print("correct")
        else:
            print("incorrect")
    else:
        break
