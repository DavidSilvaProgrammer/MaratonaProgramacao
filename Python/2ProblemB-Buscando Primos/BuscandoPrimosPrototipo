lista_nome=[]
lista_numero=[]

n=int(input())

for i in range(n):
    entrada = input("Digite o nome e o nÃºmero: ")
    partes = entrada.rsplit(' ', 1)
    nome = partes[0]
    numero = float(partes[-1])
    
    lista_nome.append(nome)
    lista_numero.append(numero)
    
    
    

print(f"{lista_nome}\n {lista_numero}")


